{
	"name": "vscode-auto-tinypng",
	"displayName": "auto-tinypng",
	"description": "",
	"version": "1.0.2",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-auto-tinypng.compress",
				"title": "Compress"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "vscode-auto-tinypng.compress",
					"when": "explorerResourceIsFolder"
				}
			],
			"commandPalette": [
				{
					"command": "vscode-auto-tinypng.compress",
					"when": "false"
				}
			]
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/lijinrong/vscode-auto-tinypng"
	},
	"publisher": "JRLI",
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.25.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}